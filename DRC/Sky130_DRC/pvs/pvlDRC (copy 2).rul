rule vhvi.1.- {
	caption "Min width of vhvi:dg >= 0.020 um"
	inte vhvi -lt 0.020 -single_point
}

rule vhvi.2.- {
	caption "Vhvi:dg cannot overlap areaid:ce"
	and vhvi areaid.cd
}

rule vhvi.3.- {
	caption "VHVGate must overlap hvi:dg"	
	select -interact -not VHVGate hvi
}

